{
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "useTabs": false,
  "trailingComma": "all",
  "semi": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/(.*)$"
  ],
  "bracketSameLine": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-css-order"
  ]
}