{
  "name": "imagepetapeta-beta",
  "version": "2.2023.700-beta",
  "private": true,
  "description": "image viewer",
  "author": "takumus",
  "license": "MIT",
  "main": "electronTemp/dist/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "generate-assets": "run-s generate-assets:*",
    "generate-assets:temp-dir": "tsx ./scripts/generate/tempDir.ts",
    "generate-assets:extraFiles": "tsx ./scripts/generate/extraFiles/index.ts",
    "generate-assets:image": "tsx ./scripts/generate/images.ts",
    "generate-assets:debuggers": "tsx ./scripts/generate/debuggers.ts",
    "generate-assets:app-icon": "tsx ./scripts/generate/appIcon.ts",
    "create-release": "tsx ./scripts/createRelease.ts",
    "dev": "vite",
    "build": "vite build && tsx ./scripts/buildElectron.ts && npm run create-release",
    "build:mac-arm64": "vite build && tsx ./scripts/buildElectron.ts --electron-target=mac:arm64 && npm run create-release",
    "build:mac-x64": "vite build && tsx ./scripts/buildElectron.ts --electron-target=mac:x64 && npm run create-release",
    "build:win": "vite build && tsx ./scripts/buildElectron.ts --electron-target=win:x64 && npm run create-release",
    "preview": "vite preview",
    "tsc": "vue-tsc --noEmit ",
    "format": "prettier ./src --write",
    "asar": "asar list",
    "tsx": "tsx",
    "prebuild": "npm run generate-assets",
    "predev": "npm run generate-assets",
    "prepare": "npm run generate-assets"
  },
  "devDependencies": {
    "@shockpkg/icon-encoder": "^2.1.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/adm-zip": "^0.5.0",
    "@types/color-convert": "^2.0.0",
    "@types/color-diff": "^1.2.1",
    "@types/cors": "^2.8.12",
    "@types/dateformat": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "18.8.0",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/recursive-readdir": "^2.2.1",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sharp": "^0.31.0",
    "@types/throttle-debounce": "^5.0.0",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "adm-zip": "^0.5.10",
    "asar": "^3.2.0",
    "electron": "24.1.2",
    "electron-builder": "^24.0.0",
    "license-checker-rseidelsohn": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "recursive-readdir": "^2.2.3",
    "sass": "^1.61.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-esmodule": "^1.4.4",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@araki-packages/promise-serial": "^2.0.2",
    "@seald-io/nedb": "^4.0.0",
    "@xmldom/xmldom": "^0.8.5",
    "axios": "^1.3.4",
    "color-convert": "^2.0.1",
    "color-diff": "^1.2.0",
    "cors": "^2.8.5",
    "data-uri-to-buffer": "^4.0.0",
    "dateformat": "^5.0.3",
    "deepcopy": "^2.1.0",
    "eventemitter3": "^5.0.0",
    "express": "^4.18.2",
    "express-ipfilter": "^1.3.1",
    "file-type": "^18.2.1",
    "fuse.js": "^6.6.2",
    "gifuct-js": "^2.1.2",
    "p-limit": "^4.0.0",
    "pixi.js": "7.2.4",
    "quantize": "^1.0.2",
    "sharp": "^0.32.0",
    "throttle-debounce": "^5.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-i18n": "v9.3.0-beta.17"
  },
