{
  "name": "imagepetapeta-beta",
  "version": "2.40.0-beta",
  "private": true,
  "description": "image viewer",
  "author": "takumus",
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build:win": "yarn prebuild && vue-cli-service electron:build --publish never --win --x64",
    "build:mac-intel": "yarn prebuild && vue-cli-service electron:build --publish never --mac --x64",
    "build:mac-arm64": "yarn prebuild && vue-cli-service electron:build --publish never --mac --arm64",
    "lint": "vue-cli-service lint",
    "generate-assets": "run-s generate-assets:*",
    "generate-assets:cursors": "node ./scripts/generateCursors.js",
    "generate-assets:dirs": "node ./scripts/generateDirs.js",
    "generate-assets:icons": "node ./scripts/generateIcons.js",
    "generate-assets:licenses": "node ./scripts/generateLicenses.js",
    "generate-assets:extra-files": "node ./scripts/generateExtraFiles.js",
    "linecount": "node ./scripts/linecount",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "postbuild:win": "node ./scripts/compressForWindows.js",
    "prebuild": "yarn audit --groups dependencies && yarn generate-assets && yarn tsc",
    "prepare": "husky install && yarn generate-assets",
    "preserve": "yarn generate-assets",
    "test": "node --loader ts-node/esm -r tsconfig-paths/register ./src/test/__test",
    "tsc": "tsc -p ./tsconfig.json --noEmit && vue-tsc --noEmit"
  },
  "dependencies": {
    "@araki-packages/promise-serial": "^2.0.2",
    "@seald-io/nedb": "^3.1.0",
    "@xmldom/xmldom": "^0.8.5",
    "axios": "^1.1.2",
    "body-parser": "^1.20.1",
    "color-convert": "^2.0.1",
    "color-diff": "^1.2.0",
    "cors": "^2.8.5",
    "data-uri-to-buffer": "^4.0.0",
    "dateformat": "^5.0.3",
    "deepcopy": "^2.1.0",
    "express": "^4.18.2",
    "express-ipfilter": "^1.3.1",
    "file-type": "^18.0.0",
    "fuse.js": "^6.6.2",
    "gifuct-js": "^2.1.2",
    "p-limit": "^4.0.0",
    "pixi.js": "^7.0.3",
    "quantize": "^1.0.2",
    "sharp": "^0.31.1",
    "throttle-debounce": "^5.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@shockpkg/icon-encoder": "^2.1.1",
    "@types/color-convert": "^2.0.0",
    "@types/color-diff": "^1.2.1",
    "@types/cors": "^2.8.12",
    "@types/dateformat": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.14",
    "@types/fuzzy-search": "^2.1.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "18.8.0",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sharp": "^0.31.0",
    "@types/throttle-debounce": "^5.0.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "adm-zip": "^0.5.9",
    "chalk": "4.1.2",
    "core-js": "^3.24.1",
    "electron": "20.3.2",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.32.7",
    "sass-loader": "^13.1.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-tsc": "^1.0.9",
    "worker-loader": "^3.0.8"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "productName": "ImagePetaPeta-beta",
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
