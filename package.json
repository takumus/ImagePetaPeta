{
  "name": "imagepetapeta-beta",
  "productName": "ImagePetaPeta-beta",
  "version": "2.7.0-beta",
  "private": true,
  "author": "takumus",
  "description": "image viewer",
  "lint-staged": {
    "*.{ts,js,vue}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prepare": "husky install && yarn generate-assets",
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "release": "yarn build && node ./scripts/commitAndAddTag.js",
    "preserve": "yarn generate-assets",
    "prebuild": "yarn tsc && yarn audit --groups dependencies && yarn generate-assets",
    "test": "node --loader ts-node/esm -r tsconfig-paths/register ./src/test/__test",
    "generate-assets": "run-s generate-assets:*",
    "generate-assets:dirs": "node ./scripts/generateDirs.js",
    "generate-assets:licenses": "node ./scripts/generateLicenses.js",
    "generate-assets:icons": "node ./scripts/generateIcons.js",
    "generate-assets:cursors": "node ./scripts/generateCursors.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "lint": "vue-cli-service lint",
    "linecount": "node ./scripts/linecount",
    "tsc": "tsc -p ./tsconfig.json --noEmit "
  },
  "main": "background.js",
  "dependencies": {
    "@araki-packages/promise-serial": "^2.0.2",
    "@seald-io/nedb": "^3.0.0",
    "@xmldom/xmldom": "^0.8.2",
    "axios": "^0.27.2",
    "data-uri-to-buffer": "^3.0.1",
    "dateformat": "^5.0.3",
    "deepcopy": "^2.1.0",
    "fuzzy-search": "^3.2.1",
    "gifuct-js": "^2.1.2",
    "pixi.js": "^6.4.2",
    "quantize": "^1.0.2",
    "sharp": "^0.30.6",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-class-component": "8.0.0-rc.1",
    "vue-i18n": "9.2.0-beta.36",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@shockpkg/icon-encoder": "^1.3.1",
    "@types/dateformat": "^5.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fuzzy-search": "^2.1.2",
    "@types/mime-types": "^2.1.1",
    "@types/nedb": "^1.8.12",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sharp": "^0.28.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "chalk": "4.1.2",
    "core-js": "^3.24.1",
    "electron": "19.0.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "ts-loader": "~8.2.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "worker-loader": "^3.0.8"
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  }
}
